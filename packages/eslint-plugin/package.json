{
    "name": "@master/eslint-plugin-css",
    "type": "module",
    "scripts": {
        "build": "techor build \"src/**/*.{js,ts}\"",
        "dev": "pnpm build --watch",
        "test": "vitest",
        "type-check": "tsc --noEmit",
        "lint": "eslint src"
    },
    "keywords": [
        "mastercss",
        "eslint",
        "eslintplugin"
    ],
    "license": "MIT",
    "description": "Master CSS ESLint Plugin",
    "author": "Aoyue Design LLC.",
    "funding": "https://rc.css.master.co/sponsor",
    "homepage": "https://css.master.co",
    "bugs": {
        "url": "https://github.com/master-co/css/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/master-co/css.git",
        "directory": "packages/eslint-plugin"
    },
    "sideEffects": false,
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "jsnext:main": "./dist/index.mjs",
    "esnext": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "dependencies": {
        "@master/css": "workspace:^",
        "@master/css-explore-config": "workspace:^",
        "@master/css-validator": "workspace:^",
        "@typescript-eslint/utils": "^7.7.0",
        "synckit": "^0.9.0",
        "typescript-eslint": "^7.7.0"
    },
    "devDependencies": {
        "@angular-eslint/template-parser": "^17.3.0",
        "@master/css-explore-config": "workspace:^",
        "shared": "workspace:^",
        "@types/node": "^20.12.7",
        "@typescript-eslint/rule-tester": "^7.7.0",
        "eslint": "^8.57.0",
        "eslint-plugin-markdown": "^4.0.1",
        "eslint-plugin-mdx": "^3.1.5",
        "svelte-eslint-parser": "^0.34.1",
        "ts-node": "^10.9.2",
        "vue-eslint-parser": "^9.4.2"
    }
}