{
    "name": "@master/css-language-service",
    "type": "module",
    "scripts": {
        "build": "techor build \"src/**/*.ts\"",
        "dev": "pnpm build --watch",
        "type-check": "tsc --noEmit",
        "lint": "eslint src",
        "test": "jest"
    },
    "description": "Master CSS Language Service",
    "author": "Aoyue Design LLC.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/master-co/css.git",
        "directory": "packages/language-service"
    },
    "dependencies": {
        "@babel/parser": "^7.23.9",
        "@master/css": "workspace:^",
        "color-convert": "^2.0.1",
        "color-rgba": "^3.0.0",
        "color-space": "^2.0.1",
        "explore-config": "^3.0.18",
        "js-beautify": "^1.15.1",
        "minimatch": "^9.0.3",
        "vscode-css-languageservice": "^6.2.12"
    },
    "devDependencies": {
        "@types/chroma-js": "^2.4.4",
        "@types/color-convert": "^2.0.3",
        "@types/color-rgba": "^2.1.2",
        "@types/vscode": "^1.86.0",
        "vscode-languageserver": "^7.0.0",
        "vscode-languageserver-textdocument": "^1.0.11",
        "vscode-languageserver-types": "^3.17.5"
    },
    "sideEffects": false,
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "jsnext:main": "./dist/index.mjs",
    "esnext": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./settings": {
            "import": "./dist/settings/index.mjs",
            "require": "./dist/settings/index.cjs",
            "types": "./dist/settings/index.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "./": [
                "./dist/index.d.ts"
            ],
            "./settings": [
                "./dist/settings.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}